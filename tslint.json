{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "no-unused-expression": true
    },
    "rules": {
        "whitespace": false,
        "jsdoc-format": true,
        "prefer-for-of": false,
        "forin": false,
        "align": true,
        "new-parens": false,
        "no-angle-bracket-type-assertion": false,
        "no-console": false,
        "no-namespace": false,
        "eofline": false,
        "quotemark": [
            true,
            "single"
        ],
        "indent": false,
        "member-access": [
            false
        ],
        "ordered-imports": [
            false
        ],
        "max-line-length": [
            true,
            280
        ],
        "member-ordering": [
            false
        ],
        "curly": false,
        "interface-name": [
            false
        ],
        "array-type": [
            false
        ],
        "no-empty-interface": false,
        "no-empty": false,
        "arrow-parens": false,
        "object-literal-sort-keys": false,
        "no-unused-expression": false,
        "max-classes-per-file": false,
        "variable-name": [
            false
        ],
        "one-line": [
            false
        ],
        "one-variable-per-declaration": [
            false
        ]
    },
    "rulesDirectory": []
}